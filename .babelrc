{
  "presets": ["next/babel"],
  "plugins": [
    [
      "import", {
        "libraryName": "antd",
        // "libraryDirectory": "lib",
        "style": true
      }
    ],
    "lodash",
    ["module-resolver", {
      "cwd": "babelrc",
      "root": ["./src"],
      "extensions": [".js"],
      "alias": {
        "@": "./src"
      }
    }]
  ]
}
